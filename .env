# PostgreSQL Settings
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=my-super-secret-password

# n8n Security Keys
# For production, generate these with a command like: openssl rand -hex 32
N8N_ENCRYPTION_KEY=change-me-to-a-long-random-string-for-security
N8N_USER_MANAGEMENT_JWT_SECRET=change-me-too-to-a-different-long-random-string